@import "../../../vector/icon/iconfont.wxss";

page {
  padding: 10px;
  width: calc(100% - 20px);
  font-size: 15px;
  background: #fff;
}

.fun {
  width: 25%;
  font-size: 13px;
  text-align: center;
  margin-top: 10px;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}

.imgIcon {
  width: 30px;
  height: 30px;
  padding: 7px;
  border: 1px solid #79b2f9;
  border-radius: 30px;
  align-self: center;
}

.icon {
  width: 30px;
  height: 30px;
  color: #4c98f7;
  font-size: 30px;
  padding: 7px;
  border: 1px solid #79b2f9;
  border-radius: 30px;
  align-self: center;
}

i {
  display: block;
}

